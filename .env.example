# browser port for remote debugging (default: 4969)
RPP_BROWSER_PORT=4969

# development server URL (default: http://localhost:5173)
RPP_DEV_SERVER_URL=http://localhost:5173

# gh personal access token for fetching presences
# RPP_GITHUB_TOKEN=ghp_your_token_here

# custom Steam config path
# RPP_STEAM_CONFIG=C:\Program Files (x86)\Steam\config\config.vdf
